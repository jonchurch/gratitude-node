  <% include partials/scripts %>
  <script src="/assets/js/sign-in.js"></script>
<script>
  $(document).ready(function() {
   const id=$.urlParam('id');
   //set time then forward to info
   setTimeout(function(){
     window.location.assign("/info/?id="+id);
          // $("#info-link").attr("href",'/info/?id='+_id);
    }, 4000);
    $("#info-link").attr("href",'/info/?id='+id);
  
 
});

  </script>
  <% include partials/cookie-msg %>
